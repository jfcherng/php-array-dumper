{
    "name": "jfcherng/php-array-dumper",
    "description": "Dump an array into XML, JSON, YAML, etc...",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Jack Cherng",
            "email": "jfcherng@gmail.com"
        }
    ],
    "keywords": [
        "JSON",
        "XML",
        "YAML"
    ],
    "autoload": {
        "psr-4": {
            "Jfcherng\\ArrayDumper\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Jfcherng\\ArrayDumper\\Test\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "require": {
        "php": "^7.1.3",
        "nicmart/string-template": "^0.1",
        "spatie/array-to-xml": "^2.7",
        "symfony/yaml": "^4.2"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.14.1",
        "phan/phan": "^1.2",
        "phpunit/phpunit": "^7.2 || ^8.0"
    },
    "scripts": {
        "analyze": [
            "phan --color"
        ],
        "fix": [
            "php-cs-fixer fix --verbose"
        ],
        "test": [
            "phpunit --verbose"
        ]
    }
}
